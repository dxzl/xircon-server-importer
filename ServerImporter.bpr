VERSION = BCB.04.04
# ---------------------------------------------------------------------------
PROJECT = ServerImporter.exe
OBJFILES = ServerImporter.obj MainForm.obj
RESFILES = ServerImporter.res
RESDEPEN = $(RESFILES) MainForm.dfm
LIBFILES =
LIBRARIES = dclocx40.lib Vclx40.lib Vcl40.lib
SPARELIBS = Vcl40.lib Vclx40.lib dclocx40.lib
PACKAGES = Vcl40.bpi Vclx40.bpi vcljpg40.bpi bcbsmp40.bpi Qrpt40.bpi Vcldb40.bpi \
  ibsmp40.bpi vcldbx40.bpi TeeUI40.bpi teedb40.bpi tee40.bpi nmfast40.bpi \
  dclocx40.bpi TaeRichEditComponent.bpi
DEFFILE =
# ---------------------------------------------------------------------------
PATHCPP = .;
PATHASM = .;
PATHPAS = .;
PATHRC = .;
DEBUGLIBPATH = $(BCB)\lib\debug
RELEASELIBPATH = $(BCB)\lib\release
USERDEFINES =
SYSDEFINES = NO_STRICT
# ---------------------------------------------------------------------------
CFLAG1 = -IServerImporter;$(BCB)\include;$(BCB)\include\vcl -O2 -Hc \
  -H=$(BCB)\lib\vcl40.csm -w -Ve -a8 -k- -vi -c -b- -w-par -w-inl -Vx -tW -tWM \
  -D$(SYSDEFINES);$(USERDEFINES)
PFLAGS = -UServerImporter;$(BCB)\Projects\Lib;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -IServerImporter;$(BCB)\include;$(BCB)\include\vcl -$Y- -$L- -$D- -v -JPHNE -M
RFLAGS = -iServerImporter;$(BCB)\include;$(BCB)\include\vcl
AFLAGS = /iServerImporter /i$(BCB)\include /i$(BCB)\include\vcl /mx /w2 /zn
LFLAGS = -LServerImporter;$(BCB)\Projects\Lib;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -aa -Tpe -x -Gn
# ---------------------------------------------------------------------------
ALLOBJ = c0w32.obj sysinit.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES) $(LIBRARIES) import32.lib cp32mt.lib
